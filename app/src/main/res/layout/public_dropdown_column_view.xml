<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <fj.dropdownmenu.lib.view.DropdownColumnView
        android:id="@+id/lvType"
        app:columnSelectedIcon="@mipmap/ic_check2"
        app:columnType="singleRow"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" />

    <fj.dropdownmenu.lib.view.DropdownColumnView
        android:id="@+id/lvSubject"
        app:columnType="randomView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" />

    <fj.dropdownmenu.lib.view.DropdownColumnView
        android:id="@+id/lvMultiple"
        app:columnType="singleRow"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" />

    <fj.dropdownmenu.lib.view.DropdownColumnView
        android:id="@+id/lvCondition"
        app:columnSelectedIcon="@mipmap/ic_check2"
        app:columnType="randomView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" />
</merge>